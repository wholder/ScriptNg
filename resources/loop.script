// Example ScriptNg code

function sum (a, b)
	return a + b
end

println("loop 3 times")
for (jj = 0; jj < 3; jj++)
	println("jj == " + jj)
end

println("loop 5 times")
ii = 0
while (ii < 5)
	print("ii == " + ii + " - ")
	if ((ii & 1) == 1)
	    print("odd ")
	elif (ii == 2)
	    print(" bingo! ")
	else
	    print("even ")
	ii = sum(ii, 1)
	println("")
end
return "done"